{
  "name": "flxuicomponents",
  "version": "0.1.2",
  "description": "Vue 3 UI Component Library mit Tailwind CSS",
  "type": "module",
  "main": "./dist/ui-components.umd.js",
  "module": "./dist/ui-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-components.es.js",
      "require": "./dist/ui-components.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/vue3-vite": "^9.1.13",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "playwright": "^1.56.1",
    "storybook": "^9.1.13",
    "tailwindcss": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "tailwind",
    "component-library"
  ],
  "author": "Florian Ehmann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EhmannFlo/UiComponents"
  }
}
